@model string

@if (Model == "Ouro")
{
    <img class="img-selo" src="~/Content/Images/Ouro.png" />
}
else if (Model == "Prata")
{
    <img class="img-selo" src="~/Content/Images/Prata.png" />
}
else if (Model == "Bronze")
{
    <img class="img-selo" src="~/Content/Images/Bronze.png" />
}
else if (Model == "Ouro-lg")
{
    <img class="img-selo" src="~/Content/Images/Ouro-lg.png" />
}
else if (Model == "Prata-lg")
{
    <img class="img-selo" src="~/Content/Images/Ouro-lg.png" />
}
else if (Model == "Bronze-lg")
{
    <img class="img-selo" src="~/Content/Images/Ouro-lg.png" />
}


